<table>
  <th>Name</th>
  <th>Quantity</th>
  <% @cart_items.each do |cart_item| %>
  <tr>
    <td class="title-#{cart_item.id}"><%= cart_item.title %></td>
    <td class="quantity-#{cart_item.id}"><%= cart_item.quantity %></td>
    <td class="image-#{cart_item.id}"><%= image_tag("#{cart_item.image_url}" , size: "30")%></td>
  </tr>
  <% end %>
</table>
