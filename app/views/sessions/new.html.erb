<h1>Log In Here</h1>

<%= form_tag login_path do %>
 <%= label_tag :username %>
 <%= text_field_tag :username %>
 <%= label_tag :password %>
 <%= password_field_tag :password %>
 <%= label_tag :first_name %>
 <%= text_field_tag :first_name %>
 <%= label_tag :last_name %>
 <%= text_field_tag :last_name %>
 <%= label_tag :address %>
 <%= text_field_tag :address %>
 <%= submit_tag "Log In" %>
<% end %>
