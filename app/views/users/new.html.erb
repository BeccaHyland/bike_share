<h1>Sign up to be a new user</h1>

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br/>
  <%= f.label :password %>
  <%= f.password_field :password %><br/>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br/>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br/>
  <%= f.label :address %>
  <%= f.text_field :address %><br/>
  <%= f.submit %>
<% end %>
